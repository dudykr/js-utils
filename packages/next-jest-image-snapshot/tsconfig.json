{
  "compilerOptions": {
    "target": "es2019",             
    "useDefineForClassFields": true,
    "module": "commonjs",                        
    "baseUrl": "./",                             
    "paths": {},                                 
    "resolveJsonModule": true,                   
    "declaration": true,
    "sourceMap": true, 
    "outDir": "./out", 
    "downlevelIteration": true,  
    "esModuleInterop": true,                  
    "forceConsistentCasingInFileNames": true, 
    "strict": true,                                  
    "skipLibCheck": true                                
  }
}
